{
  "manifest_version": 2,
  "name": "Discord Rich Presence",
  "version": "0.0.1",
  "description": "",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_icon": "icons/icon16.png"
  },
  "permissions": ["background", "tabs"]
}
